
#150112

mkdir -p ../link-grammar_NLS/usr/share/link-grammar

[ -d usr/share/link-grammar/ar ] && mv -f usr/share/link-grammar/ar ../link-grammar_NLS/usr/share/link-grammar/ar
[ -d usr/share/link-grammar/de ] && mv -f usr/share/link-grammar/de ../link-grammar_NLS/usr/share/link-grammar/de
[ -d usr/share/link-grammar/fa ] && mv -f usr/share/link-grammar/fa ../link-grammar_NLS/usr/share/link-grammar/fa
[ -d usr/share/link-grammar/he ] && mv -f usr/share/link-grammar/he ../link-grammar_NLS/usr/share/link-grammar/he
[ -d usr/share/link-grammar/lt ] && mv -f usr/share/link-grammar/lt ../link-grammar_NLS/usr/share/link-grammar/lt
[ -d usr/share/link-grammar/ru ] && mv -f usr/share/link-grammar/ru ../link-grammar_NLS/usr/share/link-grammar/ru
[ -d usr/share/link-grammar/tr ] && mv -f usr/share/link-grammar/tr ../link-grammar_NLS/usr/share/link-grammar/tr

