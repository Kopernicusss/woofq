#! /bin/bash

export MAIN_DIALOG='
<vbox>
  <frame Pixmap from Stock>
    <pixmap icon_size="6">
      <input file stock="gtk-info"></input>
    </pixmap>
  </frame>
  <frame Pixmap from File>
    <pixmap>
      <input file>gvim.png</input>
    </pixmap>
  </frame>
  <hbox>
    <button cancel></button>
    <button ok></button>
  </hbox>
</vbox>
'

gtkdialog3 --program=MAIN_DIALOG

