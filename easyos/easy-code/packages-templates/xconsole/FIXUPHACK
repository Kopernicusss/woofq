
#20200927
#20230312 append "NoDisplay=true" as in menu just confuses users.
mkdir -p usr/share/applications
echo '[Desktop Entry]
Encoding=UTF-8
Name=Xconsole monitor console
Icon=mini-sh1.xpm
Comment=Xconsole monitor console /dev/console
Exec=xconsole
Terminal=false
Type=Application
Categories=TerminalEmulator
GenericName=Xconsole
NoDisplay=true' > usr/share/applications/xconsole.desktop
