#200514 support quirky

mkdir -p etc/mtpaint

if [ "$WOOF_VARIANT" == "easy" ];then
 echo 'docsBrowser = /usr/local/bin/defaultbrowser
last_dir = /files/media/images
fact5Command = gtklp %f
' > etc/mtpaint/mtpaintrc
else
 echo 'docsBrowser = /usr/local/bin/defaultbrowser
last_dir = /file/media/images
fact5Command = gtklp %f
' > etc/mtpaint/mtpaintrc
fi
