#included in create-skel script.

#these are "orange ball" menu entries, not installed but in the kirkstone pkg repo.
#not appropriate for easyvoid as pkgget doesn't install from kirkstone repo.
for aR in $(grep -G -l '^Exec=.*\.install' ${E1}/rootfs-skeleton/usr/share/applications/*.desktop)
do
 [ -z "$aR" ] && continue
 aF="${aR##*/}" #ex: obs.desktop
 rm -f ${E1}/rootfs-skeleton/usr/share/applications/${aF}
 if [ -e ${E1}/rootfs-skeleton/usr/bin/${aF/.desktop/}.install ];then
  rm -f ${E1}/rootfs-skeleton/usr/bin/${aF/.desktop/}.install
 fi
done

