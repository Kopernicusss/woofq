#easyvoid

mkdir -p usr/bin

#keep only these (requires bash)...
GLOBIGNORE='blkid:cfdisk:col:column:eject:fallocate:fdisk:flock:fsck:getopt:hwclock:ipcs:logger:losetup:lsblk:lscpu:mkfs:mkfs.cramfs:mkfs.minix:mkswap:nsenter:rename:rev:setarch:setsid:setterm:sfdisk:swapoff:swapon:uuidgen:whereis'
cd usr/bin
rm *
cd ../../
unset GLOBIGNORE
